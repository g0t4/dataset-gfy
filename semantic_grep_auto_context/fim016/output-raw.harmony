        except StopIteration:
            current_index = -1
        # Cycle to the next zoom level
        next_index = (current_index + 1) % len(FIXED_ZOOM_LEVELS_SECONDS)
        new_width = FIXED_ZOOM_LEVELS_SECONDS[next_index]
        middle = (cur_xlim[0] + cur_xlim[1]) / 2
        half = new_width / 2
        plotter.ax.set_xlim(middle - half, middle + half)
        plotter.toggle_zoom_level_elements()
        plotter.fig.canvas.draw_idle()
